<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextArea?>
<BorderPane fx:id="mainBorderPane" style="-fx-background-color: #ddffff"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="myti.ViewAllJourneysDialogController" prefHeight="600" prefWidth="500">
    <center>
        <VBox>
            <padding>
                <Insets bottom="10" left="10" right="10" top="20"/>
            </padding>
            <TextArea fx:id="journeyTextArea" wrapText="true" prefHeight="550">
                <font>
                    <Font size="14.0" />
                </font>
            </TextArea>
        </VBox>
    </center>
</BorderPane>
