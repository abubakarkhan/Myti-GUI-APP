<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<BorderPane fx:id="mainBorderPane" style="-fx-background-color: #ddffff"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="myti.TravelStatisticsDialogController">
    <center>
        <VBox>
        <padding>
            <Insets bottom="10" left="10" right="10" top="20" />
        </padding>
        <Label fx:id="statisticsLabel" textAlignment="JUSTIFY">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        </VBox>
    </center>
    </BorderPane>
